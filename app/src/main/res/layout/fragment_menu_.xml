                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Hola"
                android:textStyle="bold"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="¿Que te gustaria conocer?"
                android:textStyle="bold"
                android:textSize="20sp" />
        </LinearLayout>

        <ScrollView
